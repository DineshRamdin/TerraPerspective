@{
	Layout = "_BlankLayout";
	ViewData["Title"] = "Preview";
}
@model List<BL.Models.Administration.UserDTO>
@using UI.Resources

<div class="outer_main">
	<main class="">
		<div class="report">
			@if (Model.Count > 0)
			{
				@if (@Model[0].ProfileImagebase64 == null)
				{
					<div>
						<img src="~/image/default-profile.png" />
					</div>
				}
				else
				{
					<div>
						<img src="data:image/png;base64,@Model[0].ProfileImagebase64" />
					</div>
				}
			}
			else
			{
				<label>@SharedResource.Image_Not_found</label>
			}

		</div>

	</main>
</div>
