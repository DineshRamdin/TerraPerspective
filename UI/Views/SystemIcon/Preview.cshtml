@{
	Layout = "_BlankLayout";
	ViewData["Title"] = "Preview";
}
@model List<BL.Models.Administration.SystemIconDTO>
@using UI.Resources


<div class="outer_main">
	<main class="">
		<div class="report">
			@if (Model.Count > 0)
			{
				@if (@Model[0].SystemIconImagebase64 == null)
				{
					@* <div style="text-align:center; padding-top: 60px; margin-right: 140px;">
						<label>Image Not found</label>
					</div> *@
					<div>
						<img src="~/image/default-profile.png" />
					</div>
				}
				else
				{
					<div>
						<img src="data:image/png;base64,@Model[0].SystemIconImagebase64" />
					</div>
				}
			}
			else
			{
				<label>@SharedResource.Image_Not_found</label>
			}

		</div>

	</main>
</div>
